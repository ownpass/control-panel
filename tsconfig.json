{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": false,
        "removeComments": false,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es6",
            "es2015",
            "dom"
        ],
        "sourceMap": true,
        "pretty": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitUseStrict": false,
        "noFallthroughCasesInSwitch": true,
        "outDir": "./build/compiled-ts/",
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules"
        ],
        "types": [
        ]
    },
    "files": [
        "app/main.ts"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "src"
    ],
    "compileOnSave": false
}
